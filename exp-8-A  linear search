# include <stdio.h>

int main(){

	int array[100],search,i,n;	

	printf("enter the elemnets in array:\n");

	scanf("%d",&n);

	

	printf("enter %d integers:\n",n);

	

	for (i=0;i<n;i++)

	{

		scanf("%d",&array[i]);

	}

	printf("enter the element to be search :\n");

	scanf("%d",&search);

	

	for (i=0;i<n;i++)

	{

		if (array[i]==search)

		{

		printf("%d is present in the array at location %d", search, i+1);

		break;

		}	

		

		if (i==n) 

		printf("%d is mot present in the given array:\n",search);

		

	}

	return 0;

}
